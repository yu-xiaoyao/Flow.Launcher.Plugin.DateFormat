<?xml version="1.0"?>

<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="plugin_title">Date Time Formatter</system:String>
    <system:String x:Key="plugin_desp">a plugin for date,time,datetime</system:String>

    <system:String x:Key="currentTimestamp">CurrentTimestamp(ms)</system:String>
    <system:String x:Key="currentTimestampSecond">CurrentTimestamp(s)</system:String>

    <system:String x:Key="timestampTitle">Timestamp(ms)</system:String>
    <system:String x:Key="timestampSecondTitle">Timestamp(s)</system:String>
    <system:String x:Key="timestampDayTitle">Timestamp(day)</system:String>

