    <system:String x:Key="todaySecondTitle">Second</system:String>
    <system:String x:Key="todayMillsSecondTitle">Millisecond</system:String>


    <system:String x:Key="unitDayTitle">Day</system:String>
    <system:String x:Key="unitHourTitle">Hour</system:String>
    <system:String x:Key="unitMinuteTitle">Minute</system:String>
    <system:String x:Key="unitSecondTitle">Second</system:String>
    <system:String x:Key="unitMillSecondTitle">Millisecond</system:String>

